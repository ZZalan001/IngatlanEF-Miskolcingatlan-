<Window x:Class="IngatlanEF.IngatlanWindow.IngatlanFelvetelWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IngatlanEF.IngatlanWindow"
        mc:Ignorable="d"
        Title="IngatlanFelvetelWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Label Content="Település"/>
            <TextBox Name="tbxTelepules"/>
            <Label Content="Cím"/>
            <TextBox Name="tbxCim"/>
            <Label Content="Ár"/>
            <TextBox Name="tbxAr"/>
            <Label Content="Típus"/>
            <ComboBox Name="cbxTipus"/>
            <Label Content="Kép név"/>
            <TextBox Name="tbxKepNev"/>
            <Label Content="Ügyintéző id"/>
            <ComboBox Name="cbxUgyintezo"/>
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Margin="10">
                <Button Name="btnMentes" Content="Mentés" Click="btnMentes_Click"/>
                <Button Name="btnMegsem" Content="Mégsem"/>
            </StackPanel>
        </StackPanel>
    </Grid>
</Window>
