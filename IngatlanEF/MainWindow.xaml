<Window x:Class="IngatlanEF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IngatlanEF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Menu>
            <MenuItem Header="Belépés"/>
            <MenuItem Header="Ingatlanok" IsEnabled="False">
                <MenuItem Header="Ingatlanok listája"/>
                <MenuItem Header="Ingatlanok felvitele"/>
                <MenuItem Header="Ingatlanok módisítása"/>
                <MenuItem Header="Ingatlanok törlése"/>
            </MenuItem>
            <MenuItem Header="Ügyintézők" IsEnabled="False">
                <MenuItem Header="Ügyintézők listája"/>
                <MenuItem Header="Ügyintézők felvitele"/>
                <MenuItem Header="Ügyintézők módisítása"/>
                <MenuItem Header="Ügyintézők törlése"/>
            </MenuItem>
        </Menu>
    </Grid>
</Window>
